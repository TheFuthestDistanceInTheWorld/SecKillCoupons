<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.dao.CouponDao">
 	<insert id="insertACoupon">
 	insert into coupons(id,user_id,min,discount,begin_date,end_date) values (#{coupon.id},#{coupon.user_id},
 	#{coupon.min},#{coupon.discount},#{coupon.begin_date},#{coupon.end_date});
 	</insert>
</mapper>
